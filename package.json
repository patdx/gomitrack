{
  "name": "gomitrack",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "start": "wireit",
    "typecheck": "wireit",
    "typegen": "wireit",
    "deploy": "wireit",
    "postinstall": "pnpm typegen"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "1.22.1",
    "@react-router/node": "7.13.0",
    "@react-router/serve": "7.13.0",
    "@turf/bbox": "7.3.4",
    "@turf/helpers": "7.3.4",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "immer": "10.2.0",
    "isbot": "5.1.35",
    "lodash": "4.17.23",
    "lucide-react": "0.575.0",
    "luxon": "3.7.2",
    "maplibre-gl": "5.18.0",
    "mingo": "6.7.2",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-map-gl": "8.1.0",
    "react-router": "7.13.0",
    "rrule": "2.8.1",
    "tailwind-merge": "3.5.0",
    "url-join": "5.0.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "0.2.3",
    "@playwright/mcp": "0.0.68",
    "@react-router/dev": "7.13.0",
    "@react-router/fs-routes": "7.13.0",
    "@tailwindcss/vite": "4.2.0",
    "@types/lodash": "4.17.23",
    "@types/luxon": "3.7.1",
    "@types/node": "24.10.13",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "autoprefixer": "10.4.24",
    "esbuild": "0.27.3",
    "postcss": "8.5.6",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "shadcn": "3.8.5",
    "tailwindcss": "4.2.0",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "unplugin-auto-import": "20.3.0",
    "vite": "7.3.1",
    "vite-tsconfig-paths": "5.1.4",
    "wireit": "0.14.12",
    "workerd": "1.20260228.0",
    "wrangler": "4.61.1"
  },
  "wireit": {
    "build": {
      "command": "react-router build",
      "dependencies": [
        "typecheck"
      ],
      "files": [
        "app/**",
        "vite.config.ts",
        "react-router.config.ts"
      ],
      "output": [
        "build/**"
      ]
    },
    "deploy": {
      "command": "wrangler deploy",
      "dependencies": [
        "build"
      ]
    },
    "dev": {
      "command": "react-router dev",
      "files": [
        "app/**",
        "vite.config.ts"
      ],
      "service": true
    },
    "start": {
      "command": "wrangler dev",
      "service": true
    },
    "typecheck": {
      "command": "tsc --build",
      "dependencies": [
        "typegen"
      ],
      "files": [
        "tsconfig.json",
        "app/**/*.ts",
        "app/**/*.tsx",
        "worker-configuration.d.ts",
        ".react-router/**"
      ],
      "output": [
        "tsconfig.tsbuildinfo"
      ]
    },
    "typegen": {
      "command": "node scripts/update-date.js && wrangler types && react-router typegen",
      "files": [
        "app/**",
        "react-router.config.ts",
        ".env",
        "wrangler.json"
      ],
      "output": [
        "worker-configuration.d.ts",
        ".react-router/**"
      ]
    }
  },
  "packageManager": "pnpm@10.30.1",
  "pnpm": {
    "overrides": {
      "esbuild": "$esbuild",
      "@esbuild-plugins/node-globals-polyfill": "$@esbuild-plugins/node-globals-polyfill",
      "workerd": "$workerd"
    },
    "onlyBuiltDependencies": []
  }
}
